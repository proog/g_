{
  "name": "g_",
  "version": "1.0.0",
  "description": "A game collection web app",
  "main": "",
  "dependencies": {
    "uglify-js": "latest",
    "angular-loading-bar": "~0.7.0",
    "chart.js": "~1.0.1",
    "moment": "~2.9.0",
    "angular-bootstrap": "~0.12.1",
    "angular-upload": "~1.0.11"
  },
  "devDependencies": {},
  "scripts": {
    "vendor:clean": "rm -rf public/vendor",
    "vendor:copy": "mkdir public/vendor && cp node_modules/{angular-bootstrap/ui-bootstrap-tpls.min.js,angular-loading-bar/build/loading-bar.min.js,angular-loading-bar/build/loading-bar.min.css,angular-upload/angular-upload.min.js,chart.js/Chart.min.js,moment/min/moment.min.js} public/vendor",
    "js:uglify": "cd public/js && uglifyjs app.js */*.js -o g.min.js --source-map g.min.js.map",
    "build": "npm run vendor:clean && npm run vendor:copy && npm run js:uglify",
    "postinstall": "npm run build"
  },
  "author": "Per Mortensen",
  "license": "ISC"
}
